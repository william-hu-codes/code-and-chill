
<%- include("../partials/header") %>
<table>
  <thead>
    <tr>
      <th>Brand</th>
      <th>Potassium</th>
      <th>Phosphate</th>
      <th> </th>
    </tr>
  </thead>

  <tbody>
    <% allFluids.forEach(f=>{%>
    <tr>
      <td><%= f.brand %></td>
      <td><%= f.potassium %></td>
      <td><%= f.phos %></td>
      <td>
        <form action="/fluids/<%=f._id%>?_method=DELETE" method="POST">
        <input type="hidden" name="_method" value="DELETE">
        <button type="submit">Remove Fluid</button>
        </form>
       </td>
    </tr>

    <%})%>
    
  </tbody>

</table>

<%- include("../partials/footer") %>
